    " def open_video(code):\n",
    "    # 자막 열기\n",
    "    a = WebDriverWait(driver, 30).until(EC.presence_of_element_located((By.CSS_SELECTOR,'#info-contents > ytd-video-primary-info-renderer')))\n",
    "    time.sleep(3)\n",
    "    b = a.find_elements_by_css_selector('#button > yt-icon')[4].click()\n",
    "    time.sleep(1)\n",
    "    try :     \n",
    "        # 자막버튼을 찾는다.\n",
    "        driver.find_element_by_css_selector('#items > ytd-menu-service-item-renderer:nth-child(2) > paper-item > yt-formatted-string').click()\n",
    "        data = []\n",
    "        # 자막이 있다면 자막을 저장한다.\n",
    "        c = WebDriverWait(driver, 30).until(EC.presence_of_element_located((By.CSS_SELECTOR,'#body > ytd-transcript-body-renderer')))\n",
    "        data.append(c.text)\n",
    "        save_csv(data,code)\n",
    "    # 자막변환이 없으면 그냥 패스\n",
    "    except Exception as e:            \n",
    "        pass\n",
    "    time.sleep(1)\n",
    "    #자막 수집\n",
    "\n",
    "    "
   ]
  },
